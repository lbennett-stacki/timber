{
  "name": "@mdrxtech/timber",
  "version": "1.3.15",
  "description": "MDRxTech's logging library",
  "source": "src/index.ts",
  "main": "dist/node/timber.umd.js",
  "module": "dist/node/timber.mjs",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/node/timber.mjs",
      "require": "./dist/node/timber.umd.js",
      "types": "./dist/node/index.d.ts"
    },
    "./node": {
      "import": "./dist/node/timber.mjs",
      "require": "./dist/node/timber.umd.js",
      "types": "./dist/node/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser/timber.mjs",
      "require": "./dist/browser/timber.umd.js",
      "types": "./dist/browser/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm build:node && pnpm build:browser",
    "build:node": "vite build --config vite.node.config.mts",
    "build:browser": "vite build --config vite.browser.config.mts",
    "build:tsc": "tsc",
    "dev": "vite",
    "dev:tsc": "pnpm build:tsc --watch",
    "lint": "eslint --fix ./src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "release": "pnpm build && changeset publish",
    "visualize": "vite-bundle-visualizer",
    "test": "vitest"
  },
  "license": "UNLICENSED",
  "repository": "https://github.com/mdrxtech/timber",
  "author": "MDRxTech Engineering <engineering@mdrx.tech>",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/cli-progress": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^5.0.7",
    "vite-bundle-visualizer": "^0.11.0",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "deepmerge": "^4.3.1",
    "superjson": "^2.2.0"
  },
  "gitHead": "5bd2ddee2dae86ccd30513331e9a675eed90bc16"
}
